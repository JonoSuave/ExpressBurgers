extends layout


block content
    
    div(class="container")
        div(class="row")
            each fry in fries
                div(class="col-sm col-lg-4 text-center")
                    h5= fry.fry
                    img(src=fry.imageUrl class="fry-img")
                    h6= fry.price
                    p #{fry.description}